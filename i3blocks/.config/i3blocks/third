[public_ip]
command=curl -s ifconfig.me | xargs echo
interval=60
label=IP: 
color=#027806

[dns_server]
command=~/.config/i3blocks/scripts/is_ip_up.sh 192.168.10.10
interval=60
label=DNS: 

